{
    "name": "tkhamez/eve-srp",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EveSrp\\": "src",
            "Test\\": "tests"
        },
        "files": ["src/functions.php"]
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "bravecollective/neucore-api": "^1.4",
        "bryanjhv/slim-session": "^4.0",
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^2.7",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.2",
        "symfony/cache": "^5.3",
        "tkhamez/eve-sso": "^3.0",
        "tkhamez/slim-role-auth": "^4.0",
        "twig/twig": "^3.1",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "post-install-cmd": [
            "bin/doctrine orm:generate-proxies"
        ]
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "sort-packages": true
    }
}
